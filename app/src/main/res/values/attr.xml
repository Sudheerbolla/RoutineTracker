<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppTextView">
        <attr name="txt_font_type" format="integer">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
            <enum name="italic" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="AppEditText">
        <attr name="edt_font_type" format="integer">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
            <enum name="italic" value="3" />
        </attr>
        <attr name="set_input_type" format="boolean" />
    </declare-styleable>

</resources>